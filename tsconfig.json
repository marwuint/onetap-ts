{   
    "exclude": [ "node_modules" ],
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "esModuleInterop": true,
        "allowJs": true,
        "checkJs": true,
        
        "strict": true,
        "alwaysStrict": true,

        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "forceConsistentCasingInFileNames": true,
    }
}