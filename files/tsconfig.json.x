{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "target": "ES5",
        "lib": [
            "es5"
        ],
        "outDir": "../scripts",
        "module": "CommonJS",
        "esModuleInterop": true,
        "allowJs": true,
        "checkJs": true,
        "noImplicitUseStrict": true,
        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "forceConsistentCasingInFileNames": true
    }
}